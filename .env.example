# PhishGard Environment Variables Example
# Copy this file to .env and fill in the values for production

# --- Database Configuration ---
POSTGRES_USER=phishgard_user
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=phishgard_db

# --- OSINT API Keys ---
IPINFO_API_KEY=your_ipinfo_api_key_here
ABUSEIPDB_API_KEY=your_abuseipdb_api_key_here

# --- Security Keys (Generate new ones for production) ---
FERNET_KEY=generate_new_fernet_key_here
JWT_SECRET_KEY=generate_new_jwt_secret_here

# --- Google OAuth Configuration ---
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# --- Application Configuration ---
FRONTEND_URL=https://your-domain.here # default: http://localhost:8000
CALLBACK_DOMAIN=your-callback-domain.here # default: localhost/api/auth/callback or your production domain